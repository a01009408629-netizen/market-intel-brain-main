# Market Intel Brain - Ingestion Engine Environment Variables
# Copy this file to .env and fill in your actual API keys

# ===========================================
# BINANCE CONFIGURATION
# ===========================================
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# ===========================================
# FINNHUB CONFIGURATION
# ===========================================
FINNHUB_API_KEY=your_finnhub_api_key_here

# ===========================================
# ALPHA VANTAGE CONFIGURATION
# ===========================================
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# ===========================================
# NEWSAPI CONFIGURATION
# ===========================================
NEWSAPI_API_KEY=your_newsapi_api_key_here

# ===========================================
# INGESTION ENGINE CONFIGURATION
# ===========================================
# Maximum number of worker tasks
INGESTION_MAX_WORKERS=100

# Normalization buffer queue size
INGESTION_QUEUE_SIZE=10000

# Batch processing size
INGESTION_BATCH_SIZE=100

# Queue flush interval in seconds
INGESTION_FLUSH_INTERVAL=1.0

# P95 latency target in milliseconds
INGESTION_P95_LATENCY_TARGET_MS=100.0

# Target requests per second
INGESTION_THROUGHPUT_TARGET=10000

# Global connection limit
INGESTION_GLOBAL_CONNECTION_LIMIT=5000

# Keep-alive timeout in seconds
INGESTION_KEEPALIVE_TIMEOUT=30.0

# Connection cleanup interval in seconds
INGESTION_CONNECTION_CLEANUP_INTERVAL=300.0

# Enable performance metrics
INGESTION_ENABLE_METRICS=true

# Metrics collection interval in seconds
INGESTION_METRICS_INTERVAL=60.0

# Logging level (DEBUG, INFO, WARNING, ERROR)
INGESTION_LOG_LEVEL=INFO

# Global circuit breaker timeout in seconds
INGESTION_GLOBAL_CIRCUIT_TIMEOUT=300.0

# Global failure threshold percentage (0.0-1.0)
INGESTION_FAILURE_THRESHOLD_PERCENTAGE=0.1

# ===========================================
# SOURCE-SPECIFIC SETTINGS
# ===========================================

# Binance Settings
BINANCE_REQUESTS_PER_SECOND=20
BINANCE_REQUESTS_PER_MINUTE=1200
BINANCE_CONNECT_TIMEOUT=5.0
BINANCE_READ_TIMEOUT=10.0

# Yahoo Finance Settings
YAHOO_FINANCE_REQUESTS_PER_SECOND=2
YAHOO_FINANCE_REQUESTS_PER_MINUTE=100
YAHOO_FINANCE_CONNECT_TIMEOUT=10.0
YAHOO_FINANCE_READ_TIMEOUT=30.0

# Finnhub Settings
FINNHUB_REQUESTS_PER_SECOND=5
FINNHUB_REQUESTS_PER_MINUTE=300
FINNHUB_CONNECT_TIMEOUT=10.0
FINNHUB_READ_TIMEOUT=30.0

# Alpha Vantage Settings
ALPHA_VANTAGE_REQUESTS_PER_SECOND=5
ALPHA_VANTAGE_REQUESTS_PER_MINUTE=500
ALPHA_VANTAGE_CONNECT_TIMEOUT=10.0
ALPHA_VANTAGE_READ_TIMEOUT=30.0

# NewsAPI Settings
NEWSAPI_REQUESTS_PER_SECOND=10
NEWSAPI_REQUESTS_PER_MINUTE=1000
NEWSAPI_CONNECT_TIMEOUT=10.0
NEWSAPI_READ_TIMEOUT=30.0
